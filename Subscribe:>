game:GetService("Players").LocalPlayer.Character.Dragonslayer.Event:FireServer(unpack(v52));
		wait(0.05);
	end
end
function doDestroyer()
	while getgenv().Destroyer == true do
		local v53 = {[1]=1};
		game:GetService("Players").LocalPlayer.Character:FindFirstChild("Universe Destroyer").Event:FireServer(unpack(v53));
		wait(0.05);
	end
end
function doBlade()
	while getgenv().Blade == true do
		local v54 = {[1]=3};
		game:GetService("Players").LocalPlayer.Character:FindFirstChild("Ancient Dragon Blade").Event:FireServer(unpack(v54));
		wait(0.05);
	end
end
